18=create view citta_dist as\r\n select distinct citta, '1' as tipo from inserzioni where tipo='1'\r\n union\r\n select distinct citta, '2' as tipo from inserzioni where tipo='2'\r\n union\r\n select distinct citta, '3' as tipo from inserzioni where tipo='3'
